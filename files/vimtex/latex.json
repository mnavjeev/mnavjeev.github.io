	"Short Template":{
		"prefix": "tshort",
		"body":[
			"\\documentclass[10pt]{article}",
			"",
			"% Packages with options",	
			"\\usepackage[english]{babel}",
			"\\usepackage[mathscr]{euscript}",
			"\\usepackage[margin=1in]{geometry}",
			"\\usepackage[utf8]{inputenc}",
			"\\usepackage[small]{titlesec}",
			"",
			"% Primary Packages",
			"\\usepackage{adjustbox, amsbsy, amsmath, amssymb, amsthm, bm, commath, chngcntr, dsfont, econometrics, fancyhdr, gensymb, graphicx, IEEEtrantools, longtable, marginnote, mathrsfs, mathtools, mdframed, natbib, parskip, pgf, setspace, subfigure, tabularx, textcomp, tikz}",
			"",
			"% Hyperref Setup",
			"\\usepackage[pdfauthor={${3:Manu Navjeevan}},",
			"\t\t\tbookmarks=false,%",
			"\t\t\tpdftitle={$2},%",
			"\t\t\tpdftoolbar=false,%",
			"\t\t\tpdfmenubar=true]{hyperref} %hyperref needs to be last",
			"",
			"% Rest of the setup is in the \"manu/manu_long\" package",
			"\\usepackage{manu${1:_long}}",
			"",
			"%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
			"",
			"\\title{$2} %Title",
			"\\author{${3:Manu Navjeevan}}",
			"\\date{${4:\\today}}",
			"",
			"\\begin{document}",
			"\\maketitle",
			"$0",
			"",
			"",
			"\\end{document}"	
		],
