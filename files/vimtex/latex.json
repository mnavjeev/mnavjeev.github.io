{
 	// Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"Math Inline":{
		"prefix": "mk",
		"body":[
			"\\($1\\)$0"
		],
		"description": "Math Inline"
	},
	"Display Math":{
		"prefix": "dm",
		"body": [
			"\\[",
			"\t\t$1",
			"\\]"
		],
		"description": "Display Math"
	},
	"Figure":{
		"prefix": "fig",
		"body": [
		"\\begin{figure}[${1:htbp}]",
		"\t\\centering",
		"\t\\includegraphics[width=${2:0.8}\\linewidth]{$3}"
		"\t\\caption{$4}",
		"\t\\label{$5}",
		"\\end{figure}",
		"$0"
		],
		"description": "Figure"
	},
	"Description":{
		"prefix": "desc",
		"body": [
			"\\begin{description}",
			"\t\\item[$1] $0",
			"\\end{description}"
		]
	},
	"Individual Item":{ 
		"prefix":"it",
		"body":"\\item $0"
	},
	"Section":{ 
		"prefix":"sec",
		"body":[ 
			"\\section{$1}%",
			"\\label{sec:$2}",
			"$0"
		]
	},
	"Section*":{ 
		"prefix":"sec*",
		"body":"\\section*{$0}"
	},
	"Subsection":{ 
		"prefix": "subsec",
		"body":[ 
			"\\subsection{$1}", 
			"\\label{subsec:$2}",
			"$0",
		]
	},
	"Paragraph":{ 
		"prefix": "par",
		"body":[ 
			"\\paragraph{$1}", 
			"\\label{par:$2}",
			"$0",
		]
	},
	"Paragraph*":{ 
		"prefix": "par*",
		"body":[ 
			"\\paragraph*{$1}", 
			"$0",
		]
	},
	"Emph":{
		"prefix": "emph",
		"body": "\\emph{$0}"
	},
	"Bold Text":{
		"prefix": "bf",
		"body": "\\textbf{$0}"
	},
	"Italic Text":{
		"prefix": "ita",
		"body": "\\textit{$0}"
	},
	"Underlined Text":{
		"prefix": "und",
		"body": "\\underline{$0} $0"
	},
	"Bar":{
		"prefix": "bar",
		"body": "\\overline{$1} $0"
	},
	"Fraction":{
		"prefix": "frac",
		"body": "\\frac{$1}{$2}$0"
	},
	"Norm":{
		"prefix": "\\|",
		"body": "\\|$0\\|"
	},
	"Left Right Norm":{
		"prefix":"\\lnm",
		"body": "\\left\\|$0\\right\\|"
	},
	"Left Right Angle":{
		"prefix": "\\lang",
		"body": "\\langle $0\\rangle",
		"description": "Inner Product"
	},
	"Left Bracket":{
		"prefix": "\\lb",
		"body": "\\left[$0\\right]"
	},
	"Left Curly Bracket":{	
		"prefix": "\\lcb",
		"body": ["\\left\\{$0\\right\\}"]
	},
	"Left Parantheses":{
		"prefix": "\\lp",
		"body": "\\left($0\\right)"
	},
	"Theorem":{
		"prefix":"thm",
		"body": [
			"\\begin{theorem}[$1]", 
			"\t\\label{thm:$2}",
			"\t$0",
			"\\end{theorem}"
		]
	},
	"Theorem*":{
